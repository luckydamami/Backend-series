<%- include('../partials/head') %>
<body>
  <%- include('../partials/nav') %>
  <main>
    <h1 class="text-center">Register your home on Airbnb</h1>
    <form action="/add-home" method="post" class="form">
      <div class="form-group">
        <input
          type="text"
          name="houseName"
          placeholder="Enter Home Name"
          required
        />
        <input
          type="text"
          name="location"
          placeholder="Enter Location"
          required
        />
      </div>
      <div class="form-group">
        <input
          type="number"
          name="rooms"
          placeholder="Enter No. of Rooms"
          required
        />
        <input type="number" name="price" placeholder="Enter Price" required />
      </div>
      <div class="form-group">
        <input type="number" name="ratings" placeholder="Give Home Ratings" />
        <input type="url" name="photoUrl" placeholder="Enter Photo URL" />
      </div>
      <div class="form-group">
        <input type="submit" value="Submit" class="btn" />
      </div>
    </form>
  </main>
</body>
